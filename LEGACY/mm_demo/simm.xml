    <channel>
        <shadowing bn="-110.0dBm" syncbits="8">
		RP(d) = received power at distance d
		XP    = transmitted power
		X     = lognormal random Gaussian component
 		=======================================================
		RP(d)/XP [dB] = -10 x 5.1 x log(d/1.0m) + X(1.0) - 33.5
 		=======================================================
	</shadowing>
	<cutoff>-115.0dBm</cutoff>
